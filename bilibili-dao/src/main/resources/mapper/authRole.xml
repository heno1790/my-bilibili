<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--DAO层是直接与数据库进行交互的层,xml文件自然是写道这一层模块中了-->
<mapper namespace="com.tt.dao.AuthRoleDao">

    <select id="getRoleByCode" parameterType="java.lang.String" resultType="com.tt.domain.auth.AuthRole">
        select * from t_auth_role where code = #{code}
    </select>
</mapper>

