<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tt.dao.FileDao">
    <insert id="addFile" parameterType="com.tt.domain.File">
        insert into
            t_file(
            url,
            type,
            md5,
            createTime
        )values(
                   #{url},
                   #{type},
                   #{md5},
                   #{createTime}
               )
    </insert>
    <select id="getFileByMD5" parameterType="java.lang.String" resultType="com.tt.domain.File">
        select
            *
        from
            t_file
        where
            md5=#{md5}
    </select>
</mapper>